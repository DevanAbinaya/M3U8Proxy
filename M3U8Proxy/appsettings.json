{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://proxy.vnxservers.com/"
      },
      "Https": {
        "Url": "https://proxy.vnxservers.com/",
        "Certificates": {
          "Default": {
            "Path": "/etc/letsencrypt/live/proxy.vnxservers.com/fullchain.pem",
            "KeyPath": "/etc/letsencrypt/live/proxy.vnxservers.com/privkey.pem"
          }
        }
      }

    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Host": "http://proxy.vnxservers.com/",
            "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://localhost:5001"
          }
        }
      }
    }
  }
  
}

