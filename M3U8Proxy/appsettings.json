  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "catchallroute" : {
        "ClusterId": "minimumcluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "minimumcluster": {
        "Destinations": {
          "proxy.vnxservers.com": {
            "Address": "https://proxy.vnxservers.com/"
          }
        }
      }
    }
  },
  "LettuceEncrypt": {
    
    "AcceptTermsOfService": true,

    "DomainNames": [ "proxy.vnxservers.com" ],

    "EmailAddress": "toxiccheart12@gmail.com"
  }
}
