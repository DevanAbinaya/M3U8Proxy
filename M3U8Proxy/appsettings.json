{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LettuceEncrypt": {
    
    "AcceptTermsOfService": true,

    "DomainNames": [ "proxy.vnxservers.com","9anime.vnxservers.com" ],

    "EmailAddress": "toxiccheart12@gmail.com"
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}",
          "Host": "proxy.vnxservers.com"
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "{**catch-all}",
          "Host": "9anime.vnxservers.com"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "http://localhost:5000/"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "cluster2/destination1": {
            "Address": "http://localhost:3061/"
          }
        }
      }
    }
  }
}

