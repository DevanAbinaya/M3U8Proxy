{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://proxy.vnxservers.com"
      },
      "Https": {
        "Url": "http://proxy.vnxservers.com"
      },
      "Certificates": {
        "Default": {
          "Path": "/etc/letsencrypt/live/proxy.vnxservers.com/fullchain.pem",
          "KeyPath": "/etc/letsencrypt/live/proxy.vnxservers.com/privkey.pem"
        }
      }
    }
  }
}
