{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:80"
      },
      "HttpsFromPem": {
        "Url": "https://0.0.0.0:443",
        "Certificates": {
            "Path": "/etc/letsencrypt/live/proxy.vnxservers.com/fullchain.pem",
            "KeyPath": "/etc/letsencrypt/live/proxy.vnxservers.com/privkey.pem"
          
        }
      }

    }
  }
  

  
}

